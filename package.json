{
  "name": "pokedex",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Pokedex App",
  "author": "Roger Baracy <rogerbaracy.s@gmail.com>",
  "private": true,
  "scripts": {
    "publish": "quasar build && ./node_modules/.bin/gh-pages -d dist/spa",
    "test:report": "./generate_report_test.sh",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test:chrome": "cypress run --browser chrome",
    "report:merge": "mochawesome-merge cypress/results/json/*.json > cypress/results/json/index.json",
    "report:generate": "marge cypress/results/json/index.json --reportDir cypress/results/report/ --assetsDir cypress/results/report/assets --reportPageTitle index.html",
    "dev": "quasar dev",
    "build": "quasar build",
    "cypress:open": "cypress open -d",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@quasar/babel-preset-app": "^2.0.1",
    "@quasar/extras": "^1.10.2",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "quasar": "^1.15.9",
    "vue-class-component": "^7.2.2",
    "vue-i18n": "^8.0.0",
    "vue-property-decorator": "^8.3.0"
  },
  "devDependencies": {
    "@cypress/vue": "^2.1.1",
    "@quasar/app": "^2.2.3",
    "@quasar/quasar-app-extension-dotenv": "^1.1.0",
    "@types/node": "^10.17.15",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.0.1",
    "cypress": "^7.0.1",
    "cypress-multi-reporters": "^1.4.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.1.2",
    "gh-pages": "^3.1.0",
    "mocha": "^8.3.2",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "prettier": "2.2.1",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
